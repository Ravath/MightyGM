<UserControl x:Class="MightyGM.Tabs.TabFiches"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MightyGM.Tabs"
             xmlns:tkg="clr-namespace:Xceed.Wpf.Toolkit.PropertyGrid;assembly=Xceed.Wpf.Toolkit"
             xmlns:core="clr-namespace:MightyGM.GUIcore"
             xmlns:coredata="clr-namespace:Core.Data;assembly=Core"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <DockPanel x:Name="xDock">
        <ListView x:Name="xTypeList" DockPanel.Dock="Left" SelectionChanged="xTypeList_SelectionChanged" MinWidth="100"/>
        <UserControl x:Name="xContainer"/>
    </DockPanel>
</UserControl>
