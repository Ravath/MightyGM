<UserControl x:Class="CinqAnneauxWpf.Fiches.FicheCandidateCapacites"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CinqAnneauxWpf.Fiches"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <TabControl>
        <TabItem Header="Katas">
            <local:FicheListeKatas x:Name="xKatas"/>
        </TabItem>
        <TabItem Header="Kihos">
            <local:FicheListeKihos x:Name="xKihos"/>
        </TabItem>
        <TabItem Header="Pouvoirs Outremonde">
            <local:FicheListePouvoirOutremonde x:Name="xPouvoirs"/>
        </TabItem>
    </TabControl>
</UserControl>
